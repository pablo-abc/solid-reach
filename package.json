{
  "name": "root",
  "private": true,
  "version": "0.0.1",
  "description": "Port of ReachUI to Solid",
  "scripts": {
    "site:dev": "pnpm --filter='@solid-reach/site' dev",
    "site:build": "pnpm --filter='@solid-reach/site' build",
    "site:preview": "pnpm --filter='@solid-reach/site' start",
    "dev": "pnpm --parallel --filter='!@solid-reach/site' dev",
    "build": "pnpm --filter='!@solid-reach/site' build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --docs -o docs",
    "prepublishOnly": "pinst --disable",
    "publish": "pnpm i && pnpm -r publish --no-git-checks --publish-branch main",
    "postpublish": "pinst --enable"
  },
  "author": "Pablo Berganza <pablo@berganza.dev>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-typescript": "^7.14.5",
    "@changesets/cli": "^2.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@solid-reach/rollup-plugin-jsx": "workspace:*",
    "@types/node": "^16.3.1",
    "acorn-jsx": "^5.3.2",
    "babel-preset-solid": "^1.0.1",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "cross-env": "^7.0.3",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "solid-js": "^1.0.2",
    "tsc-watch": "^4.4.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/html": "^6.3.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "rollup-plugin-ts": "^1.4.0",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3"
  }
}
