{
  "name": "@solid-reach/tabs",
  "version": "0.0.1",
  "description": "A tabs component",
  "main": "dist/index.js",
  "sideEffects": ["*.css"],
  "author": "Pablo Berganza <pablo@berganza.dev>",
  "repository": "github:pablo-abc/solid-reach",
  "homepage": "https://github.com/pablo-abc/solid-reach/tree/main/packages/reporter-solid",
  "keywords": ["solidjs", "solid", "a11y", "components"],
  "license": "MIT",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/index.jsx",
      "default": "./dist/index.js"
